{
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"prefer-const": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": true,
		"no-angle-bracket-type-assertion": true,
		"no-var-keyword": true
	},
	"defaultSeverity": "warning"
}